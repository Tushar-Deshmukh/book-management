{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "rewrites": [
    { "source": "/api-docs", "destination": "/api-docs/index.html" },
    { "source": "/swagger-ui-assets/:path*", "destination": "/swagger-ui-assets/:path*" },
    { "source": "/swagger.json", "destination": "/swagger.json" }
  ]
}